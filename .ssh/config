Host ec2
	User edd
	ServerAliveInterval 120
	
Host ec2proxy
	User edd
	Hostname ec2
	Port 443
	PreferredAuthentications publickey
	ProxyCommand /usr/bin/corkscrew $http_proxy 8080 %h %p

Host githubproxy
	User git
	Hostname ssh.github.com
	Port 443
	PreferredAuthentications publickey
	ProxyCommand /usr/bin/corkscrew $http_proxy 8080 %h %p

Host phone
	User root
	StrictHostKeyChecking no

